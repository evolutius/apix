{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "excludeExternals": true,
  "plugin": [
    "typedoc-github-theme"
  ],
  "projectDocuments": ["documentation/*.md"],
  "navigation": {
    "includeCategories": true
  },
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "html",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "tsx",
    "typescript",
    "nginx",
    "apache",
    "dockerfile",
    "kotlin"
  ]
}
